{
    "name": "basecodeoy/pathfinder",
    "description": "A package to easily make us of more powerful route pattern matching in Laravel.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "BaseCode Oy",
        "laravel",
        "pathfinder"
    ],
    "authors": [
        {
            "name": "Brian Faust",
            "email": "hello@basecode.sh",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/basecodeoy/pathfinder",
    "support": {
        "issues": "https://github.com/basecodeoy/pathfinder/issues",
        "source": "https://github.com/basecodeoy/pathfinder",
        "security": "https://github.com/basecodeoy/pathfinder/blob/main/.github/SECURITY.md"
    },
    "require": {
        "php": "^8.3",
        "ext-mbstring": "*",
        "basecodeoy/crate": "^1.0.0",
        "illuminate/contracts": "^11.0",
        "spatie/regex": "^3.1.1"
    },
    "require-dev": {
        "basecodeoy/standards": "^1.1.1"
    },
    "autoload": {
        "psr-4": {
            "BaseCodeOy\\Pathfinder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "infection/extension-installer": true,
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        },
        "audit": {
            "abandoned": "report"
        },
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "aliases": {
                "Pathfinder": "BaseCodeOy\\Pathfinder\\Facades\\Pathfinder"
            }
        }
    }
}
